import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int c=sc.nextInt();
    
    if(b<=a && c<=a){
        System.out.println("a is Greater");
    }
    else if(a<=b && c<=b){
        System.out.println("b is Greater");
    }
    else
    {
        System.out.println("c is Greater");

    }

}
}
